<apex:page controller="WrapperClassExample" >
    <apex:form >
        <apex:pageBlock >
            <apex:pageBlockSection >
                <apex:pageBlockTable value="{!ListToDispayWrapper}" var="wr"> 
                <apex:column headerValue="action">
                    <apex:inputCheckbox />
                </apex:column>
                <apex:column headerValue="AccName">
                    {!wr.AName}
                </apex:column>
                <apex:column headerValue="ContName">
                    {!wr.CName}
                </apex:column>
                </apex:pageBlockTable> 
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>